      "parameters": [
        {
          "name": "state",
          "description": "Two-letter state code (e.g., CA, TX, NY)",
          "type": "string",
          "optional": false
        }
      ],
      "result": {
        "type": "number",
        "dimensionality": "scalar"
      },
      "options": {
        "stream": true,
        "cancelable": true
      }
    },
    {
      "id": "OILPRICE.CODES",
      "name": "OILPRICE.CODES",
      "description": "Returns all available commodity codes, names, and categories from the API",
      "helpUrl": "https://www.oilpriceapi.com/docs",
      "parameters": [],
      "result": {
        "type": "string",
        "dimensionality": "matrix"
      }
    }
  ]
}
